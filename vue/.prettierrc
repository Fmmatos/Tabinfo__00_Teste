{
	"tabWidth": 4,
	"singleQuote": true,
	"printWidth": 120,
	"endOfLine": "lf",
	"semi": true,
	"trailingComma": "es5",
	
	"overrides": [
		{
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		},
		{
			"files": "*.ts",
			"options": {
				"parser": "typescript"
			}
		},
		{
			"files": "*.js",
			"options": {
				"parser": "babel"
			}
		}
	]
}